{
    "name": "vue3prj-test",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "prepare": "husky install",
        "lint": "lint-staged"
